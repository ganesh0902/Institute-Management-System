server:
  port: 8999
  
  
eureka:
  instance:
    hostname: localhost
    
spring:
  application:
    name: API GATEWAY
  cloud:
    gateway:
      mvc:
        routes:
        - id: batch-service
          uri: lb:batch-service
          predicates:
          - Path=/user/**
        - id: course-service
          uri: lb://course-service
          predicates:
          - Path=/contact/**
             
  
