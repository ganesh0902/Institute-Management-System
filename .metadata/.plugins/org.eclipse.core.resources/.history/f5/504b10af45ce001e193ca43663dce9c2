package com.batch.entities;

import java.util.ArrayList;
import java.util.List;

import lombok.AllArgsConstructor;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;
import lombok.ToString;

@Setter
@Getter
@ToString

public class Batch {

	private int bId;
	private String batchTitle;
	private String duration;
	private String startDate;
	private String endDate;
	private String status;
	private String location;
	private String time;
	private String image;
	private int teacherId;
	private int courseId;
	List<Course> course=new ArrayList<>();
	public Batch(int bId, String batchTitle, String duration, String startDate, String endDate, String status,
			String location, String time, String image, int teacherId, int courseId, List<Course> course) {
		super();
		this.bId = bId;
		this.batchTitle = batchTitle;
		this.duration = duration;
		this.startDate = startDate;
		this.endDate = endDate;
		this.status = status;
		this.location = location;
		this.time = time;
		this.image = image;
		this.teacherId = teacherId;
		this.courseId = courseId;
		this.course = course;
	}
	public Batch(int bId, String batchTitle, String duration, String startDate, String endDate, String status,
			String location, String time, String image, int teacherId, int courseId) {
		super();
		this.bId = bId;
		this.batchTitle = batchTitle;
		this.duration = duration;
		this.startDate = startDate;
		this.endDate = endDate;
		this.status = status;
		this.location = location;
		this.time = time;
		this.image = image;
		this.teacherId = teacherId;
		this.courseId = courseId;
	}
	
}